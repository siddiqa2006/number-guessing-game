#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir//user/bin/evn" ]; then
  exec "$basedir//user/bin/evn" node "$basedir/../ayesha-siddiqa__number-guessing-game/index.js" "$@"
else 
  exec /user/bin/evn node "$basedir/../ayesha-siddiqa__number-guessing-game/index.js" "$@"
fi
